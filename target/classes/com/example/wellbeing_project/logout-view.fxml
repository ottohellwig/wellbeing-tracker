<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.*?>
<VBox spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.wellbeing_project.logout.LogoutController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="50.0"/>
    </padding>
    <VBox alignment="CENTER" spacing="10.0">
        <Label fx:id="title" text="Wellbeing Tracker"/>
    </VBox>
    <VBox alignment="CENTER" spacing="10.0">
        <padding>
            <Insets left="10" top="30.0"/>
        </padding>
        <Label fx:id="subtitle" text="You have been successfully logged out."/>
    </VBox>
    <VBox alignment="CENTER" spacing="10.0">
        <padding>
            <Insets left="10.0" right="10.0"/>
        </padding>
        <Button fx:id="returnButton" onAction="#startLogin" text="Return to login."/>
    </VBox>
</VBox>

